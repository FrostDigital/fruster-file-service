		"fruster-bus": "0.7.11",
		"fruster-errors": "0.0.7",
		"fruster-health": "0.0.14",
		"fruster-log": "0.1.0",
		"mime-types": "2.1.26",
		"ms": "2.1.2",
		"multer": "1.4.2",
		"multer-s3": "2.9.0",
		"rebuild": "0.1.2",
		"request": "2.88.0",
		"s3-download-stream": "1.1.1",
		"sharp": "0.24.0",
		"uuid": "3.4.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.9",
		"@types/express": "^4.17.9",
		"@types/jasmine": "^3.5.3",
