		"@types/ms": "^0.7.31",
		"@types/multer": "^1.4.5",
		"@types/multer-s3": "^2.7.9",
		"@types/node": "^14.14.14",
		"@types/request": "^2.48.5",
		"@types/sharp": "^0.27.0",
		"@types/uuid": "^8.3.0",
		"fruster-test-utils": "0.5.1",
		"jasmine": "3.5.0",
		"jasmine-spec-reporter": "4.2.1",
		"nyc": "15.0.0",
		"request-image-size": "^2.1.0",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.1",
		"typescript": "^4.1.3"
	},
	"nyc": {
		"exclude": [
			"spec/*",
			"lib/errors.js",
			"repo/InMemoryImageCacheRepo.js",
